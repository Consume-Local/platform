<p id="notice"><%= notice %></p>

<h1>Listings</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Category</th>
      <th>Subcategory</th>
      <th>Street</th>
      <th>Street number</th>
      <th>Internal number</th>
      <th>Postal code</th>
      <th>District</th>
      <th>City</th>
      <th>Entity</th>
      <th>User</th>
      <th>Delivery service</th>
      <th>Carry out</th>
      <th>Online store</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @listings.each do |listing| %>
      <tr>
        <td><%= listing.name %></td>
        <td><%= listing.category_id %></td>
        <td><%= listing.subcategory_id %></td>
        <td><%= listing.street %></td>
        <td><%= listing.street_number %></td>
        <td><%= listing.internal_number %></td>
        <td><%= listing.postal_code %></td>
        <td><%= listing.district %></td>
        <td><%= listing.city_id %></td>
        <td><%= listing.entity_id %></td>
        <td><%= listing.user_id %></td>
        <td><%= listing.delivery_service %></td>
        <td><%= listing.carry_out %></td>
        <td><%= listing.online_store %></td>
        <td><%= link_to 'Show', listing %></td>
        <td><%= link_to 'Edit', edit_listing_path(listing) %></td>
        <td><%= link_to 'Destroy', listing, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Listing', new_listing_path %>
